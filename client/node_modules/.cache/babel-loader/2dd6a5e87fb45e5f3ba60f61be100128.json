AAA;AAAA;AAAA,8CAoBA,KAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,GAAA,SAAA,CApBA;;AAAA;AAAA;AAoBA,cAAA,IApBA,QAoBA,IApBA;AAqBA,mBAAA,OAAA,GAAA,IAAA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA,IAAA,UAxBA,sBAwBA,IAxBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAyBA,mBAAA,aAAA;AACA,mBAAA,OAAA,GAAA,IAAA;AACA,cAAA,WA3BA,GA2BA,KAAA,WAAA,CAAA,IAAA,CAAA,UAAA,CA3BA;AA4BA,cAAA,MA5BA,GA4BA,EA5BA;AA6BA,cAAA,KA7BA,GA6BA,EA7BA;AA8BA,mBAAA,GAAA,GAAA,OAAA;AA9BA;AAAA,8CAgCA,KAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,GAAA,aAAA,GAAA,IAAA,CAAA,EAAA,CAhCA;;AAAA;AAAA;AAgCA,cAAA,IAhCA,SAgCA,IAhCA;;AAiCA,kBAAA,IAAA,CAAA,KAAA,KAAA,IAAA,EAAA;AACA,gBAAA,MAAA,GAAA,IAAA,CAAA,KAAA;AACA,eAFA,MAEA;AACA,qBAAA,iBAAA,GAAA,IAAA;AACA;;AAEA,kBAAA,IAAA,CAAA,KAAA,KAAA,IAAA,EAAA;AACA,gBAAA,KAAA,GAAA,mDAAA,WAAA,GAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,EAAA,GAAA,SAAA,GAAA,IAAA,CAAA,MAAA,GAAA,QAAA,CAAA,EAAA,EAAA,MAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,eAFA,MAEA;AACA,qBAAA,iBAAA,GAAA,IAAA;AACA;;AACA,mBAAA,UAAA,GAAA;AACA,sBAAA,IAAA,CAAA,WAAA,CAAA,EADA;AAEA,2BAAA,IAAA,CAAA,WAAA,CAAA,OAFA;AAGA,+BAAA,IAAA,CAAA,WAAA,CAAA,WAHA;AAIA,6BAAA,IAAA,CAAA,WAAA,CAAA,SAJA;AAKA,4BAAA,IAAA,CAAA,WAAA,CAAA,QALA;AAMA,mCAAA,IAAA,CAAA,WAAA,CAAA,iBAAA,CAAA,aANA;AAOA,4BAAA,IAAA,CAAA,WAAA,CAAA,cAAA,CAAA,IAPA;AAQA,8BAAA,KAAA,YAAA,CAAA,IAAA,CAAA,UAAA,CARA;AASA,yBAAA,MATA;AAUA,yBAAA;AAVA,eAAA;AAYA,mBAAA,OAAA,GAAA,KAAA;AACA,mBAAA,MAAA,GAAA,IAAA;;AAzDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4DA,IAAA,MA5DA,oBA4DA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,UAAA,EAAA,GAAA,KAAA,UAAA,CAAA,EAAA;AACA,UAAA,WAAA,GAAA,KAAA,UAAA,CAAA,WAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,GAAA,aAAA,GAAA,EAAA,EAAA;AAAA,QAAA,WAAA,EAAA;AAAA,OAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,IAAA,CAAA,cAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KAxEA;AA0EA,IAAA,aA1EA,yBA0EA,IA1EA,EA0EA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,oDAAA,IAAA,CAAA,SAAA,GAAA,GAAA,GAAA,IAAA,CAAA,QAAA,EAAA,QAAA;AACA,KA5EA;AA8EA,IAAA,WA9EA,uBA8EA,UA9EA,EA8EA;AACA,aAAA,OAAA,CAAA,UAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA,KAhFA;AAkFA,IAAA,YAlFA,wBAkFA,IAlFA,EAkFA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,IAAA;;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,MAAA,IAAA,MAAA;AACA,KArFA;AAuFA,IAAA,aAvFA,2BAuFA;AAAA;;AACA;AACA,WAAA,eAAA,GAAA,KAAA;AAEA,WAAA,SAAA,CAAA,YAAA;AACA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,IAAA;AACA,OAHA;AAIA;AA/FA,GA/EA;AAkLA,EAAA,OAAA,EAAA;AACA,IAAA,OAAA,EAAA,mBAAA;AACA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,kBAAA;AACA,KAHA;AAIA,IAAA,SAAA,EAAA,mBAAA,IAAA,EAAA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,GAAA,aAAA,GAAA,IAAA,CAAA,WAAA,GAAA,QAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA,WAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,KAAA,EAAA,0BAFA;AAGA,YAAA,IAAA,EAAA,kBAAA,QAAA,CAAA,IAAA,CAAA,WAHA;AAIA,YAAA,iBAAA,EAAA,KAJA;AAKA,YAAA,gBAAA,EAAA,IALA;AAMA,YAAA,iBAAA,EAAA,MANA;AAOA,YAAA,iBAAA,EAAA,UAPA;AAQA,YAAA,gBAAA,EAAA,aARA;AASA,YAAA,cAAA,EAAA;AATA,WAAA;AAWA;;AACA,QAAA,MAAA,CAAA,cAAA;AACA,OAfA,EAeA,KAfA,CAeA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAjBA;AAkBA;AAvBA,GAlLA;AA4MA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,gBAAA,IAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,IAAA;;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AACA;AAJA;AA5MA,CAAA","sourcesContent":["<template>\n    <v-row\n        class=\"mt-n4\"\n        align=\"center\"\n        justify=\"center\"\n     >\n      <v-container fluid>\n        <v-card outlined>\n          <v-row class=\"mb-n6\">\n            <v-col cols=\"12\" md=\"4\">\n              <v-text-field \n                class=\"pl-2\"\n                v-model=\"plateNumber\"\n                label=\"License Number\" \n                outlined \n                dense\n                clearable\n                >\n              </v-text-field>\n            </v-col>\n            <v-col cols=\"12\" md=\"5\">\n              <v-combobox\n                  v-model=\"camera_select\"\n                  :items=\"cameras\"\n                  item-value=\"id\"\n                  item-text=\"name\"\n                  label=\"Camera Select\"\n                  outlined \n                  dense\n                  @change=\"getDataFromApi\"\n              ></v-combobox>\n            </v-col>\n            <v-col cols=\"12\" md=\"3\">\n                <v-combobox\n                    class=\"pr-2\"\n                    v-model=\"matching_select\"\n                    :items=\"matching\"\n                    label=\"Matching Type\"\n                    outlined \n                    dense\n                    @change=\"getDataFromApi\"\n                ></v-combobox>\n            </v-col>\n          </v-row>\n          <v-divider class=\"pa-1\"></v-divider>\n          <v-data-table\n            :headers=\"headers\"\n            :items=\"data_record\"\n            :page=\"page\"\n            :items-per-page=\"itemsPerPage\"\n            :loading=\"loading\"\n            class=\"elevation-1\"\n            @page-count=\"pageCount = $event\"\n            hide-default-footer\n          >\n          <template v-slot:item.matchingResult=\"{ item }\">\n            <div class=\"text-center\">\n              <v-chip\n                class=\"ma-2\"\n                v-if=\"item.matchingResult == null || item.matchingResult == 'otherlist'\"\n              >\n                None\n              </v-chip>\n\n              <v-chip\n                class=\"ma-2\"\n                color=\"light-blue\"\n                text-color=\"white\"\n                v-if=\"item.matchingResult == 'whitelist'\"\n              >\n                Whitelist\n              </v-chip>\n\n              <v-chip\n                class=\"ma-2\"\n                color=\"red\"\n                text-color=\"white\"\n                v-if=\"item.matchingResult == 'blacklist'\"\n              >\n                Blacklist\n              </v-chip>\n            </div>\n          </template>\n          <template v-slot:item.picName=\"{ item }\">\n            <v-img class=\"ma-1\" :src=\"'http://counting.test/storage/violation/picture/' + dateConvert(item.created_at)+ '/'+ item.picName + '.jpg'\" width=\"80\"/>\n          </template>\n          <template v-slot:item.id=\"{ item }\">\n            <v-btn class=\"ma-2\" tile color=\"success\" @click=\"openDetail(item)\">\n              <v-icon left>mdi-magnify</v-icon> Check\n            </v-btn>\n          </template>\n          <template v-slot:item.action=\"{ item }\">\n            <v-icon dark v-if=\"item.action == 0\" color=\"red\">mdi-brightness-1</v-icon>\n            <v-icon dark v-if=\"item.action == 1\" color=\"yellow\">mdi-brightness-1</v-icon>\n            <v-icon dark v-if=\"item.action == 2\" color=\"green\">mdi-brightness-1</v-icon>\n          </template>\n          <template v-slot:item.created_at=\"{ item }\">\n            {{ item.created_at | moment }}\n          </template>\n          </v-data-table>\n          <v-pagination class=\"pa-2\" v-model=\"page\" :length=\"pageCount\" :total-visible=\"7\" circle v-if=\"pageCount > 1\"></v-pagination>\n        </v-card>\n      </v-container>\n\n      <v-dialog v-model=\"dialog\" fullscreen hide-overlay transition=\"dialog-bottom-transition\">\n      <v-card>\n        <v-toolbar dark color=\"primary\">\n          <v-btn icon dark @click=\"dialog = false\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n          <v-toolbar-title>Data Detail</v-toolbar-title>\n          <v-spacer></v-spacer>\n          <v-toolbar-items>\n            <v-btn dark text @click=\"update\">Save</v-btn>\n          </v-toolbar-items>\n        </v-toolbar>\n        <v-divider></v-divider>\n        <v-card-text>\n          <v-row>\n            <v-col\n              cols=\"8\"\n              class=\"px-8\"\n            >\n              <v-tabs v-model=\"tab\" fixed-tabs class=\"py-3 mt-n2\">\n                <v-tabs-slider></v-tabs-slider>\n                <v-tab href=\"#tab-1\" class=\"primary--text\">\n                  <v-icon>mdi-image-multiple</v-icon>\n                   Picture Capture\n                </v-tab>\n                <v-tab href=\"#tab-2\" class=\"primary--text\">\n                  <v-icon>mdi-file-video</v-icon>\n                   Video Capture\n                </v-tab>\n\n              </v-tabs>\n              <v-tabs-items v-model=\"tab\">\n                <v-tab-item value=\"tab-1\"> \n                  <v-row>\n                      <v-img class=\"mt-8\" v-if=\"failed_load_image\" src=\"@/assets/no_result.gif\" max-height=\"400\"/>\n                      <v-col\n                        v-for=\"item in showDetail.image\"\n                        :key=\"item\"\n                        class=\"d-flex child-flex ma-n2 mt-n4\"\n                        cols=\"6\"\n                      >\n                        <v-card flat tile class=\"d-flex\">\n                          <zoom-on-hover :img-normal='\"data:image/jpeg;base64,\" + item'></zoom-on-hover>\n                        </v-card>\n                      </v-col>\n                  </v-row>\n                </v-tab-item>\n                <v-tab-item value=\"tab-2\"> \n                  <v-row class=\"px-2 mr-2\">\n                    <div v-if=\"failed_load_video\">\n                      <v-img class=\"mt-8\" src=\"@/assets/no_result.gif\" max-height=\"400\"/>\n                    </div>\n                    <div v-else>\n                      <vue-plyr v-if=\"renderComponent\">\n                        <video playsinline controls >\n                          <source  :src=\"showDetail.video\" type=\"video/mp4\" size=\"1080\">\n                        </video>\n                      </vue-plyr>\n                    </div>\n                  </v-row>\n                </v-tab-item>\n              </v-tabs-items>\n            </v-col>\n\n            <v-col class=\"ml-n8 mt-2\" cols=\"4\">\n              <v-text-field\n                v-model=\"showDetail.plateNumber\"\n                label=\"Nomor Polisi\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.typeOfViolation\"\n                label=\"Jenis Pelanggaran\"\n                readonly\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.created_at\"\n                label=\"Kejadian Pada\"\n                readonly\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.location\"\n                label=\"Lokasi Kejadian\"\n                readonly\n              ></v-text-field>\n              <v-select\n                v-model=\"actions_select\"\n                :items=\"actions\"\n                item-text=\"item\"\n                item-value=\"value\"\n                label=\"Tindakan\"\n              ></v-select>\n            </v-col>\n          </v-row>\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n    <v-overlay :value=\"overlay\">\n      <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\n    </v-overlay>\n    </v-row>\n</template>\n\n<script>\nimport moment from 'moment'\n\n  export default {\n    // components: {videoPlayer},\n    data () {\n      return {\n        tab: null,\n        page: 1,\n        pageCount: 0,\n        itemsPerPage: 10,\n        data_record: [],\n        loading: true,\n        dialog: false,\n        plateNumber : '',\n        cameras : [],\n        camera_select : '',\n        actions : [\n          {'item' : 'Belum ditindak', 'value' : 0 },\n          {'item' : 'Proses', 'value' : 1 },\n          {'item' : 'Selesai', 'value' : 2 },\n        ],\n        actions_select : 0,\n        matching : ['all','none', 'blacklist', 'whitelist'],\n        matching_select : ['all'],\n        showDetail : {\n          'picName' : null,\n          'plateNumber' : null,\n          'matchingResult' : null,\n          'camera' : null,\n          'longitude' : null,\n          'latitude' : null,\n          'created_at' : null,\n          'image' : [],\n          'video' : ''\n        },\n        headers: [\n          {\n            text: 'Image',\n            align: 'left',\n            sortable: false,\n            value: 'picName',\n            width: 150,\n          },\n          { text: 'License Number', value: 'plateNumber' },\n          { text: 'Violation Type', value: 'a_n_p_r_violation.violationName' },\n          { text: 'Time', value: 'created_at' },\n          { text: 'Action', value: 'id', sortable: false },\n        ],\n        failed_load_image : false,\n        failed_load_video : false,\n        overlay: false,\n        renderComponent: false,\n        default_image : '@/assets/no_result.gif'\n      }\n    },\n\n    watch: {\n      page(){\n        this.getDataFromApi()\n      },\n      plateNumber(){\n        this.getDataFromApi()\n      },\n      tab(){\n        if (this.tab == \"tab-2\") {\n          this.forceRerender()\n        }\n      },\n      dialog(){\n        if (this.dialog == false) {\n          this.failed_load_image = false\n          this.failed_load_video = false\n        }\n      }\n    },\n\n    created(){\n      this.getDataFromApi()\n      this.getCamera()\n    },\n\n    methods: {\n      async getDataFromApi () {\n        this.loading = true\n        let self = this\n        // Make a request for a user with a given ID\n        this.axios.get(process.env.VUE_APP_IP_SERVER + '/violation?page=' + this.page + '&itemsPerPage=' + this.itemsPerPage + '&plateNumber=' + this.plateNumber + '&camera=' + this.camera_select.id + '&matchingResult=' + this.matching_select)\n          .then(function (response) {\n            self.data_record = response.data.data\n            setTimeout(() => {\n              self.loading = false\n              self.pageCount = response.data.last_page\n            }, 1000)\n          })\n          .catch(function () {\n            self.loading = false\n            self.data_record = []\n          })\n      },\n\n      async getCamera() {\n        const { data } = await this.axios.get(process.env.VUE_APP_IP_SERVER + '/camera')\n        this.cameras = data\n      },\n      \n      async openDetail(item){\n        this.forceRerender()\n        this.overlay = true\n        let time_folder = this.dateConvert(item.created_at)\n        let images = []\n        let video = ''\n        this.tab = 'tab-1'\n        \n        const {data} = await this.axios.get(process.env.VUE_APP_IP_SERVER + '/violation/' + item.id)\n        if (data.image !== null) {\n            images = data.image\n        }else{\n            this.failed_load_image = true\n        }\n\n        if (data.video !== null) {\n            video = 'http://counting.test/storage/violation/record/'+time_folder+'/'+data.video.id + '.mp4?t=' + Math.random().toString(36).substr(2, 5)\n        }else{\n            this.failed_load_video = true\n        }\n        this.showDetail = {\n          'id' : data.anpr_record.id,\n          'picName' : data.anpr_record.picName,\n          'plateNumber' : data.anpr_record.plateNumber,\n          'longitude' : data.anpr_record.longitude,\n          'latitude' : data.anpr_record.latitude,\n          'typeOfViolation' : data.anpr_record.a_n_p_r_violation.violationName,\n          'location' : data.anpr_record.a_n_p_r_camera.name,\n          'created_at' : this.dateConvert2(item.created_at),\n          'image' : images,\n          'video' : video\n        }\n        this.overlay = false\n        this.dialog = true\n      },\n\n      update(){\n        let self = this\n        var id = this.showDetail.id\n        var plateNumber = this.showDetail.plateNumber\n        this.axios.put(process.env.VUE_APP_IP_SERVER + '/violation/' + id, { plateNumber: plateNumber})\n          .then(function () {\n            self.getDataFromApi()\n            self.dialog = false\n          })\n          .catch(function (error) {\n            window.console.log(error);\n        });\n      },\n\n      getCoordinate(item){\n          window.open('http://maps.google.com/maps?&z=15&mrt=yp&t=m&q='+item.longitude+',' +item.latitude, '_blank');\n      },\n\n      dateConvert(created_at){\n          return moment(created_at).format('YYYY-MM-DD');\n      },\n\n      dateConvert2(date){\n        moment.locale('id')\n        return moment(date).format('LLLL') + ' WIB';\n      },\n\n      forceRerender() {\n        // Remove my-component from the DOM\n        this.renderComponent = false;\n        \n        this.$nextTick(() => {\n          // Add the component back in\n          this.renderComponent = true;\n        });\n      }\n\n    },\n\n    sockets: {\n      connect: function (){\n          window.console.log('socket connected')\n      },\n      violation: function (data) {\n         this.axios.get(process.env.VUE_APP_IP_SERVER + '/filtering/'+data.plateNumber+'/check').then( response => {\n           if (response.data.categories == 'blacklist') {\n             this.$swal.fire({\n                type: 'error',\n                title: 'BLACKLIST PLATE DETECTED',\n                text: 'Keterangan : ' + response.data.description,\n                allowOutsideClick: false,\n                showCancelButton: true,\n                cancelButtonColor: '#d33',\n                confirmButtonText: 'Check It',\n                cancelButtonText: 'No, cancel!',\n                reverseButtons: false\n              })\n           }\n           this.getDataFromApi()\n         }).catch(function (error){\n           window.console.log(error)\n         })\n      }\n    },\n\n    filters: {\n      moment: function (date) {\n        moment.locale('id')\n        return moment(date).format('LLL');\n      },\n    }\n}\n</script>"],"sourceRoot":"src/pages/anpr"}]}