>\n    <v-row\n        align=\"center\"\n        justify=\"center\"\n     >\n      <v-container fluid>\n        <v-card outlined>\n          <v-row class=\"mb-n6\">\n            <v-col cols=\"12\" md=\"4\">\n              <v-text-field \n                class=\"pl-2\"\n                v-model=\"plateNumber\"\n                label=\"License Number\" \n                outlined \n                dense\n                clearable\n                >\n              </v-text-field>\n            </v-col>\n            <v-col cols=\"12\" md=\"5\">\n              <v-combobox\n                  v-model=\"camera_select\"\n                  :items=\"cameras\"\n                  item-value=\"id\"\n                  item-text=\"name\"\n                  label=\"Camera Select\"\n                  outlined \n                  dense\n                  @change=\"getDataFromApi\"\n              ></v-combobox>\n            </v-col>\n            <v-col cols=\"12\" md=\"3\">\n                <v-combobox\n                    class=\"pr-2\"\n                    v-model=\"matching_select\"\n                    :items=\"matching\"\n                    label=\"Matching Type\"\n                    outlined \n                    dense\n                    @change=\"getDataFromApi\"\n                ></v-combobox>\n            </v-col>\n          </v-row>\n          <v-divider class=\"pa-1\"></v-divider>\n          <v-data-table\n            :headers=\"headers\"\n            :items=\"data_record\"\n            :page=\"page\"\n            :items-per-page=\"itemsPerPage\"\n            :loading=\"loading\"\n            class=\"elevation-1\"\n            @page-count=\"pageCount = $event\"\n            hide-default-footer\n          >\n          <template v-slot:item.matchingResult=\"{ item }\">\n            <div class=\"text-center\">\n              <v-chip\n                class=\"ma-2\"\n                v-if=\"item.matchingResult == null || item.matchingResult == 'otherlist'\"\n              >\n                None\n              </v-chip>\n\n              <v-chip\n                class=\"ma-2\"\n                color=\"light-blue\"\n                text-color=\"white\"\n                v-if=\"item.matchingResult == 'whitelist'\"\n              >\n                Whitelist\n              </v-chip>\n\n              <v-chip\n                class=\"ma-2\"\n                color=\"red\"\n                text-color=\"white\"\n                v-if=\"item.matchingResult == 'blacklist'\"\n              >\n                Blacklist\n              </v-chip>\n            </div>\n          </template>\n          <template v-slot:item.picName=\"{ item }\">\n            <v-img class=\"ma-1\" :src=\"'http://127.0.0.1:8001/storage/' + dateConvert(item.created_at)+ '/'+ item.picName + '.jpg'\" width=\"80\"/>\n          </template>\n          <template v-slot:item.id=\"{ item }\">\n            <v-btn class=\"ma-2\" tile color=\"success\" @click=\"openDetail(item)\">\n              <v-icon left>mdi-magnify</v-icon> Check\n            </v-btn>\n          </template>\n          <template v-slot:item.created_at=\"{ item }\">\n            {{ item.created_at | moment }}\n          </template>\n          </v-data-table>\n          <v-pagination class=\"pa-2\" v-model=\"page\" :length=\"pageCount\" :total-visible=\"7\" circle></v-pagination>\n        </v-card>\n      </v-container>\n\n      <v-dialog v-model=\"dialog\" max-width=\"1024px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Data Detail</span>\n        </v-card-title>\n        <v-divider></v-divider>\n        <v-card-text>\n          <v-row>\n            <v-col\n              cols=\"8\"\n            >\n            <v-row>\n              <v-col\n                v-for=\"n in 4\"\n                :key=\"n\"\n                class=\"d-flex child-flex ma-n2 mt-n4\"\n                cols=\"6\"\n              >\n                <v-card flat tile class=\"d-flex\">\n                  <zoom-on-hover img-normal=\"http://127.0.0.1:8001/storage/violation/20200129113400159_1105BH.jpg\"></zoom-on-hover>\n                  <!-- <v-img\n                    src=\"http://127.0.0.1:8001/storage/violation/20200129113400159_1105BH.jpg\"\n                    class=\"grey lighten-2\"\n                  >\n                  </v-img> -->\n                </v-card>\n              </v-col>\n              </v-row>\n            </v-col>\n\n            <v-col class=\"ml-n8 mt-2\" cols=\"4\">\n              <v-text-field\n                v-model=\"showDetail.plateNumber\"\n                label=\"Nomor Polisi\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.typeOfViolation\"\n                label=\"Jenis Pelanggaran\"\n                readonly\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.created_at\"\n                label=\"Kejadian Pada\"\n                readonly\n              ></v-text-field>\n              <v-text-field\n                v-model=\"showDetail.location\"\n                label=\"Lokasi Kejadian\"\n                readonly\n              ></v-text-field>\n            </v-col>\n          </v-row>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"error darken-1\" text @click=\"dialog = false\">Close</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    </v-row>\n</template>\n\n<script>\nimport moment from 'moment'\n  export default {\n    // components: {'vue-load-image': VueLoadImage },\n    data () {\n      return {\n        page: 1,\n        pageCount: 0,\n        itemsPerPage: 10,\n        data_record: [],\n        loading: true,\n        dialog: false,\n        plateNumber : '',\n        cameras : [],\n        camera_select : '',\n        directions : ['forward', 'backward'],\n        direction_select: ['forward', 'backward'],\n        matching : ['all','none', 'blacklist', 'whitelist'],\n        matching_select : ['all'],\n        showDetail : {\n          'picName' : null,\n          'plateNumber' : null,\n          'matchingResult' : null,\n          'camera' : null,\n          'longitude' : null,\n          'latitude' : null,\n          'created_at' : null,\n        },\n        headers: [\n          {\n            text: 'Image',\n            align: 'left',\n            sortable: false,\n            value: 'anpr.picName',\n            width: 150,\n          },\n          { text: 'License Number', value: 'anpr.plateNumber' },\n          { text: 'Violation Type', value: 'typeOfViolation' },\n          { text: 'Status', value: 'anpr.matchingResult', align: 'center' },\n          { text: 'Time', value: 'created_at' },\n          { text: 'Action', value: 'id', sortable: false },\n        ],\n      }\n    },\n\n    watch: {\n      page(){\n        this.getDataFromApi()\n      },\n      plateNumber(){\n        this.getDataFromApi()\n      },\n    },\n\n    created(){\n      this.getDataFromApi()\n      this.getCamera()\n    },\n\n    methods: {\n      async getDataFromApi () {\n        this.loading = true\n        let self = this\n        // Make a request for a user with a given ID\n        this.axios.get(process.env.VUE_APP_IP_SERVER + '/violation?page=' + this.page + '&itemsPerPage=' + this.itemsPerPage + '&plateNumber=' + this.plateNumber + '&camera=' + this.camera_select.id)\n          .then(function (response) {\n            self.data_record = response.data.data\n            setTimeout(() => {\n              self.loading = false\n              self.pageCount = response.data.last_page\n            }, 1000)\n          })\n          .catch(function () {\n            self.loading = false\n            self.data_record = []\n          })\n      },\n\n      async getCamera() {\n        const { data } = await this.axios.get(process.env.VUE_APP_IP_SERVER + '/camera?system=anpr')\n        this.cameras = data\n      },\n      \n      async openDetail(item){\n        this.dialog = true\n        const { data } = await this.axios.get(process.env.VUE_APP_IP_SERVER + '/violation/' + item.id)\n        this.showDetail = {\n          'picName' : data.anpr_record.picName,\n          'plateNumber' : data.anpr_record.plateNumber,\n          'longitude' : data.anpr_record.longitude,\n          'latitude' : data.anpr_record.latitude,\n          'typeOfViolation' : data.violation.typeOfViolation,\n          'location' : data.anpr_record.camera.name,\n          'created_at' : this.dateConvert2(item.created_at),\n        }\n      },\n\n      getCoordinate(item){\n          window.open('http://maps.google.com/maps?&z=15&mrt=yp&t=m&q='+item.longitude+',' +item.latitude, '_blank');\n      },\n\n      dateConvert(created_at){\n          return moment(created_at).format('YYYY-MM-DD');\n      },\n\n      dateConvert2(date){\n        moment.locale('id')\n        return moment(date).format('LLLL') + ' WIB';\n      }\n\n    },\n\n    sockets: {\n      connect: function (){\n          window.console.log('socket connected')\n      },\n      plate_recognition: function (data) {\n         this.axios.get(process.env.VUE_APP_IP_SERVER + '/filtering/'+data.plateNumber+'/check').then( response => {\n           if (response.data.categories == 'blacklist') {\n             this.$swal.fire({\n                type: 'error',\n                title: 'BLACKLIST PLATE DETECTED',\n                text: 'Keterangan : ' + response.data.description,\n                allowOutsideClick: false,\n                showCancelButton: true,\n                cancelButtonColor: '#d33',\n                confirmButtonText: 'Check It',\n                cancelButtonText: 'No, cancel!',\n                reverseButtons: false\n              })\n           }else{\n             this.$swal.fire({\n                type: 'info',\n                title: 'WHITELIST PLATE DETECTED',\n                text: 'Keterangan : ' + response.data.description,\n                confirmButtonText: 'OK!',\n              })\n           }\n           this.getDataFromApi()\n         }).catch(function (error){\n           window.console.log(error)\n         })\n      }\n    },\n\n    filters: {\n      moment: function (date) {\n        moment.locale('id')\n        return moment(date).format('LLL');\n      }\n    }\n}\n</script>"]}]}