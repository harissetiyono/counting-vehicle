ttings</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>Camera</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item to=\"/livestream\">\n          <v-list-item-action>\n            <v-icon>mdi-wifi</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>Live Stream</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n\n      <v-list dense>\n      <v-list-group\n        v-for=\"item in items\"\n        :key=\"item.title\"\n        v-model=\"item.active\"\n        :prepend-icon=\"item.action\"\n        no-action\n      >\n        <template v-slot:activator>\n          <v-list-item-content to=\"/livestream\">\n            <v-list-item-title v-text=\"item.title\"></v-list-item-title>\n          </v-list-item-content>\n        </template>\n\n        <v-list-item\n          v-for=\"subItem in item.items\"\n          :key=\"subItem.id_cctv\"\n          :to=\"'/live/'+subItem.id_cctv\"\n        >\n          <v-list-item-content>\n            <v-list-item-title v-text=\"subItem.nama\"></v-list-item-title>\n          </v-list-item-content>\n          <v-list-item-icon>\n              <v-icon>mdi-video</v-icon>\n            </v-list-item-icon>\n        </v-list-item>\n      </v-list-group>\n    </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar\n      app\n      clipped-left\n    >\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\" />\n      <v-toolbar-title>Application</v-toolbar-title>\n    </v-app-bar>\n\n    <v-content>\n      <v-container fluid>\n        <dashboard-content> </dashboard-content>\n      </v-container>\n    </v-content>\n\n    <v-footer app>\n      <span>&copy; 2019</span>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nimport DashboardContent from \"./Content.vue\";\n  export default {\n    components: {\n      DashboardContent,\n    },\n    props: {\n      source: String,\n    },\n    data: () => ({\n      drawer: null,\n      items: [\n          {\n            action: 'mdi-video',\n            title: 'Camera List',\n            items: [],\n          }\n        ],\n    }),\n    created () {\n      this.$vuetify.theme.dark = true\n      this.initialize()\n    },\n    methods: {\n      async initialize () {\n        const { data } = await this.$http.get('http://127.0.0.1:8000/cctv')\n        this.items[0].items = data\n      },\n    }\n  }\n</script>"]}]}